# Prints out the nth Fibonacci sequence and the time taken to compute.

def fibonacci(n):
	if n < 2: return n
	return fibonacci(n - 1) + fibonacci(n - 2)

var start = Roo.clock

var n = 20
print("Fibonacci entry " + n + " is:")
print(fibonacci(n))

print("Time: " + (Roo.clock - start) + " microseconds.")
